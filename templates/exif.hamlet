$with smf <- showMaybeField "?"
  <tr>
    <td>Location
    <td>
      <a href="@?{(SearchImagesR 0,[atomAsParam TCountry exifCountry])}"><span title="Country">^{smf exifCountry}
      <span .fas .fa-caret-right .mx-1>
      <a href="@?{(SearchImagesR 0,[atomAsParam TProvince exifProvince])}"><span title="Province">^{smf exifProvince}
      <span .fas .fa-caret-right .mx-1>
      <a href="@?{(SearchImagesR 0,[atomAsParam TCity exifCity])}"><span title="City">^{smf exifCity}
      <span .fas .fa-caret-right .mx-1>
      <a href="@?{(SearchImagesR 0,[atomAsParam TLocation exifLocation])}"><span title="Location">^{smf exifLocation}
  <tr>
    <td>People
    <td>^{showSetField exifPeople}
  <tr>
    <td>Keywords
    <td>^{showSetField exifKeywords}
  <tr>
    <td>Title
    <td>^{showMaybeField "no title" exifTitle}
  <tr>
    <td>Caption
    <td>^{showMaybeField "no caption" exifCaption}
  <tr>
    <td>Equipment
    <td>
      ^{smf exifCamera} + #{lensShortName exifLens}
  <tr>
    <td>Capture time
    <td>^{smf createDate}
  <tr>
    <td>Exposure info
    <td>#{sspeed} @ #{aperture}, #{iso}
  <tr>
    <td>Focal length
    <td>#{fl}
  <tr>
    <td>Shutter count
    <td>^{showMaybeField "unknown" shutterCount}
  <tr>
    <td>Orientation
    <td>#{show exifOrientation}
  <tr>
    <td>MIME type
    <td>^{showMaybeField "unknown" exifMimeType}
  ^{showListWithHeader "Warnings" (Set.toList exifWarning)}
